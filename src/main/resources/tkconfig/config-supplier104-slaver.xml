<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <!-- TKmybatis配置 -->
        <property name="javaFileEncoding" value="UTF-8"/>
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>

        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="com.tiancan.api.repository.base.BaseMyBatisMapper"/>
        </plugin>

        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="false"/>
            <property name="suppressDate" value="true"/>

        </commentGenerator>
        <!-- 数据库链接URL、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://175.6.7.104:3306/supplier_db?tinyInt1isBit=false&amp;useUnicode=true&amp;characterEncoding=utf-8&amp;autoReconnect=true&amp;zeroDateTimeBehavior=convertToNull"
                        userId="admin" password="yuanyongkk250"/>

        <!-- 生成模型的包名和位置 -->
        <javaModelGenerator targetPackage="com.tiancan.api.client.domain" targetProject="src/main/java" />
        <!-- 生成的映射文件包名和位置 -->
        <sqlMapGenerator targetPackage="com.tiancan.api.repository.dao.slaver.mapper" targetProject="src/main/java" />
        <!-- 生成dao的包名和位置 -->
        <javaClientGenerator targetPackage="com.tiancan.api.repository.dao.slaver" targetProject="src/main/java" type="XMLMAPPER" />

        <table tableName="city" domainObjectName="CityDO" >
            <generatedKey column="id" sqlStatement="Mysql" identity="true"/>
        </table>




    </context>

</generatorConfiguration>